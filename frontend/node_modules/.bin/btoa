#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/paulo/PyCharm/Projetos/relampago/frontend/node_modules/btoa/bin/node_modules:/home/paulo/PyCharm/Projetos/relampago/frontend/node_modules/btoa/node_modules:/home/paulo/PyCharm/Projetos/relampago/frontend/node_modules:/home/paulo/PyCharm/Projetos/relampago/node_modules:/home/paulo/PyCharm/Projetos/node_modules:/home/paulo/PyCharm/node_modules:/home/paulo/node_modules:/home/node_modules:/node_modules:/home/paulo/PyCharm/Projetos/relampago/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/paulo/PyCharm/Projetos/relampago/frontend/node_modules/btoa/bin/node_modules:/home/paulo/PyCharm/Projetos/relampago/frontend/node_modules/btoa/node_modules:/home/paulo/PyCharm/Projetos/relampago/frontend/node_modules:/home/paulo/PyCharm/Projetos/relampago/node_modules:/home/paulo/PyCharm/Projetos/node_modules:/home/paulo/PyCharm/node_modules:/home/paulo/node_modules:/home/node_modules:/node_modules:/home/paulo/PyCharm/Projetos/relampago/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../btoa/bin/btoa.js" "$@"
else
  exec node  "$basedir/../btoa/bin/btoa.js" "$@"
fi
